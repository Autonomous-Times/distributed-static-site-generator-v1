aqua Deals declares *

data Deal:
    chainNetwork: string
    chainNetworkId: u64
    dealId: string
    dealIdOriginal: string
    definition: string
    timestamp: string

data Deals:
    contentWorkerV1: ?Deal
    renderWorkerV0: ?Deal
    taskWorkerV1: ?Deal
    webHostWorkerV0: ?Deal

func get() -> Deals:
    <- Deals(
        taskWorkerV1=nil,
        contentWorkerV1=nil,
        renderWorkerV0=nil,
        webHostWorkerV0=nil
    )
